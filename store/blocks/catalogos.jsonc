{
    "store.custom#catalogos": {
    "blocks": [
      "flex-layout.row#cats"
    ]
  },
"flex-layout.row#cats":{
    "children":[
        "flex-layout.col#colcats"
    ]
},
"flex-layout.col#colcats":{
    "props":{
        "blockClass" : "rowCats"
    },
    "children":[
        "rich-text#catTitle",
        "flex-layout.row#catalogos"
    ]
}, 
"rich-text#catTitle":{
    "props": {
        "text": "## NUEVOS CATÁLOGOS",
        "blockClass": "catTitle",
        "textAlignment" : "CENTER",
        "textPosition" : "CENTER",
        "fullWidth": true
    }
},
"flex-layout.row#catalogos":{  
    "children":[
        "slider-layout#catalogos"
        //"flex-layout.col#colcat1",
        //"flex-layout.col#colcat2",
        //"flex-layout.col#colcat3",
        //"flex-layout.col#colcat4"
        //"flex-layout.col#colcat5"
    ]
},

"slider-layout#catalogos": {
    "children": [
        "flex-layout.col#colcat1",
        "flex-layout.col#colcat2",
        "flex-layout.col#colcat3",
        "flex-layout.col#colcat4",
        "flex-layout.col#colcat5",
        "flex-layout.col#colcat6"

        //"flex-layout.col#colcat5"
    ],"props": {
        "itemsPerPage": {
          "desktop": 4,
          "tablet": 2,
          "phone": 1
        },
        "infinite": true,
        "showNavigationArrows": "desktopOnly",
        "showPaginationDots":"desktopOnly",
        "blockClass": ["carousel","back"],
        "autoplay":{
            "timeout": 4000,
            "stopOnHover": true
        }
      }
  },

"flex-layout.col#colcat1":{
    "children":[
        "flex-layout.row#imgcat1",
        "flex-layout.row#textcat1",
        "flex-layout.row#downlink1"
        
