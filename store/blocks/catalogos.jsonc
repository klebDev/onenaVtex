        "blockClass":"contcat"
    }
},
"flex-layout.row#imgcat1":{
    "children":[
        "image#catalogo1"
    ],"props":{
        "blockClass":"imgcolcat"
    }
},
"image#catalogo1":{
    "props":{
        "src":"assets/comodidad.webp",
        "maxHeight":319,
        "link": {
            "url": "https://www.flipsnack.com/BE66E9BBDC9/onena-temporada-oto-o-invierno-2022/full-view.html",
            "newTab": true
        }
    }
},
"flex-layout.row#textcat1":{
    "children":[
        "rich-text#contcat1"
    ],"props":{
        "blockClass": "contcatext"
    }
},
"rich-text#contcat1":{
    "props":{
        "text":"#### ONENA \n #### Otoño - Invierno \n [Link](https://www.flipsnack.com/BE66E9BBDC9/onena-temporada-oto-o-invierno-2022/full-view.html)",
        "blockClass":"txtcat"
    }
},
