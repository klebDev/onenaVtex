{
  "store.home": {
    "blocks": [
      "responsive-layout.desktop",
      "responsive-layout.tablet",
      "responsive-layout.phone"
    ]
  },
  "flex-layout.row#countdown": {
    "children": ["countdown"],
    "props": {
      "fullWidth": true,
      "blockClass": "rowCountDown",
      "horizontalAlign": "center"
    }
  },
  "responsive-layout.desktop":{
    "children":[
      "flex-layout.row#countdown",
      "list-context.image-list#SliderHome",
      "flex-layout.row#infoCardContainer",
      "rich-text#titulo1ProductosHome",
      "flex-layout.row#productosHome",
      "flex-layout.row#infoCardContainer2",
      "flex-layout.row#Container3"

      /* "info-card#home", */
      /* "rich-text#question", */
    ]
  },

  "responsive-layout.tablet":{
    "children":[
      "flex-layout.row#countdown",
      "list-context.image-list#SliderHome",
      "flex-layout.row#infoCardContainer",
      "rich-text#titulo1ProductosHome",
      "flex-layout.row#productosHome",
      "flex-layout.row#infoCardContainer2",
      "flex-layout.row#Container3"
    ]
  },

  "responsive-layout.phone":{
    "children":[
      "flex-layout.row#countdown",
      "list-context.image-list#SliderHome",
      "flex-layout.row#infoCardContainer",
      "__fold__.experimentalLazyAssets",
      "rich-text#titulo1ProductosHome",
      "flex-layout.row#productosHome",
      "flex-layout.row#infoCardContainer2",
      "flex-layout.row#Container3Movil"
      /* "flex-layout.row#Container3" */
    ]
  },

  "list-context.image-list#SliderHome": {
    "children": ["slider-layout#bannerprin"],
    "props": {
      "height": 720,
      "blockClass" : "SliderHome",
      "images": [
        {
          "image": "/arquivos/banner-hero.png",
          "mobileImage": "/arquivos/banner-hero-mobile.png"
        },
        {
          "image": "/arquivos/banner-hero.png",
          "mobileImage": "/arquivos/banner-hero-mobile.png"
        }
      ]
    },
    "title" : "Scroll Bar"
  },
  "flex-layout.row#infoCardContainer": {
    "children": ["info-card#infoCard1", "info-card#infoCard2", "info-card#infoCard3"]
  },

  "flex-layout.row#infoCardContainer2": {
    "children": ["info-card#infoCard1Container2", "info-card#infoCard2Container2"]
  },

  "flex-layout.row#infoCardContainerHomeLogos": {
    "children": [ "flex-layout.col#colLogo1Home",
                  "flex-layout.col#colLogo2Home",
                  "flex-layout.col#colLogo3Home",
                  "flex-layout.col#colLogo4Home",
                  "flex-layout.col#colLogo5Home"
                ],
    "props" : {
      "blockClass" : "logosHomeContainer"
    }
  },

  "flex-layout.row#infoCardContainerHomeLogosMobile": {
    "children": [
                  "flex-layout.col#colLogo2Home",
                  "flex-layout.col#colLogo3Home",
                  "flex-layout.col#colLogo5Home"
                ],
    "props" : {
      "blockClass" : "logosHomeContainerMobile"
    }
  },
  "flex-layout.row#Container3": {
    "children": ["info-card#infoCard1Container3", "flex-layout.row#productosHomeBlock2"]
  },

  "flex-layout.row#Container3Movil": {
    "children": ["info-card#infoCard1Container3Movil", "flex-layout.row#productosHomeBlock2"]
