  },







  "list-context.product-list#productosHome": {
    "blocks": ["product-summary.shelf"],
    "children": ["slider-layout#bestseason"],
    "props": {
      "orderBy": "OrderByTopSaleDESC"
    },
    "title" : "Productos"
  },
  "slider-layout#bestseason": {
    "props": {
      "itemsPerPage": {
        "desktop": 5,
        "tablet": 3,
        "phone": 1
