      "infinite": true,
      "fullWidth": false,
      "blockClass": "shelfProductosHome",
      "autoplay":{
        "timeout":4000,
        "stopOnHover":true
      }
    }
  },





  "list-context.product-list#productosHomeBlock2": {
    "blocks": ["product-summary.shelf"],
